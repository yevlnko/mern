{
  "restartable": "rs",
  "delay": "2500",
  "ignore": ["./node_modules", ".git"],
  "verbose": false,
  "watch": ["./webpack/*", "package.json", ".babelrc", "./source/theme/theme.scss"],
  "env": {
    "NODE_ENV": "development",
    "BUILD_ENV": "development"
  },
  "exec":
  "webpack-dev-server --hot --config=./webpack/webpack.config.babel.js --env development --disable-host-check",
  "ext": "js json"
}
